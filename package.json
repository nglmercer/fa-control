{
  "name": "fa-control",
  "version": "0.1.3",
  "description": "Robust cross-platform fa-control library for Node.js written in Rust. It also works with deno and bun.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "license": "MIT",
  "type": "commonjs",
  "keywords": [
    "fa-control",
    "node",
    "rust",
    "cross-platform",
    "audio"
  ],
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "binaryName": "fa-control",
    "targets": [
      "x86_64-pc-windows-msvc",
      "i686-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "aarch64-linux-android"
    ]
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:all": "napi build --release",
    "build:prebuilds": "napi build --platform --release --strip",
    "clean": "napi clean && cargo clean",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "check": "cargo check",
    "clippy": "cargo clippy -- -D warnings",
    "docs": "cargo doc --no-deps --open"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.5.1",
    "@types/node": "^25.0.3"
  },
  "packageManager": "bun@1.3.5"
}